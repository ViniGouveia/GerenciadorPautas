<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.vinigouveia.gerenciadorpautas">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.ClosedAgendasActivity" />
        <activity android:name=".activities.OpenedAgendasActivity" />
        <activity android:name=".activities.HelpUserActivity" />
        <activity android:name=".activities.CreateNewAgendaActivity" />
        <activity android:name=".activities.ProfileAcessActivity" />
        <activity android:name=".activities.CreateAccountActivity" />
        <activity android:name=".activities.RecoverPasswordActivity" />
        <activity android:name=".activities.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activities.AgendaActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>